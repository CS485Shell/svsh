$PATH = "/bin:/usr/bin"
$HomeDir = "/home/mylogin"
$Myfile = "fileA"
$Filebase = "paper"
$ShowTokens = "1"
run echo $PATH
cd /usr/bin
cd ./bin
cd ../misc/oldstuf
cd /usr
cd $HomeDir
run /usr/bin/wc -l $Myfile
run wc -l f1 f2 f3 <bg>
assignto $TextFile /bin/echo -e $Filebase "\b.txt"
run cat $TextFile
run /bin/ls
run ls -l -F -g -s /tmp
bye
